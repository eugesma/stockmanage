<div id="filterrific_results">
  <div class="card">
    <table class="table table-hover">
      <thead>
        <tr>
          <th><%= Store.human_attribute_name(:name) %></th>
          <th><%= Store.human_attribute_name(:users_count) %></th>
          <th><%= Store.human_attribute_name(:clients_count) %></th>
          <th><%= Store.human_attribute_name(:type) %></th>
          <th><%= Store.human_attribute_name(:created_at) %> <%= fa_icon "calendar" %></th>
          <th><%= Store.human_attribute_name(:updated_at) %> <%= fa_icon "calendar" %></th>
          <th></th>
        </tr>
      </thead>
      <tbody id="stores">
        <%= render @stores %>
      </tbody>
    </table>
  </div>
  <div id="page_links">
    <%= will_paginate @stores, list_classes: %w(pagination justify-content-center), renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
  </div>
</div>
